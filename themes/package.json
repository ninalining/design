  "devDependencies": {
    "normalize.css": "^8.0.1",
    "sass": "^1.81.0",
    "stylelint": "^16.10.0",
    "stylelint-config-sass-guidelines": "^12.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.1"
  }
}
